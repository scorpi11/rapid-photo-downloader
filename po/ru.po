msgid "Backing up error"
msgstr "Ошибка резервного копирования"

#: rapid/rapid.py:1716
#, python-format
msgid ""
"Destination directory could not be created\n"
"%(directory)s\n"
"Error: %(errno)s %(strerror)s"
